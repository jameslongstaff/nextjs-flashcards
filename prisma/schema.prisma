// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pack {
  id String @id
  title String
  cards Card[]
}

model Card {
  id String @id
  title String
  content String
  packId String
  pack Pack @relation(fields: [packId], references: [id])
}